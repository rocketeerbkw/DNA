              <xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"></xs:any>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PREVIEWBODY" minOccurs="0">
          <xs:complexType mixed="true">
            <xs:sequence>
              <xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"></xs:any>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="INREPLYTO" minOccurs="0">
