<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:include schemaLocation="SiteOption.xsd"/>
	<xs:element name="SITEOPTIONS">
		<xs:annotation>
			<xs:documentation>A collection of site options.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SITEOPTION" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="SITEID" type="xs:int" use="optional" />
		</xs:complexType>
	</xs:element>
</xs:schema>
